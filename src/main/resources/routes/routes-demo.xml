

<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://camel.apache.org/schema/spring"
    xsi:schemaLocation="http://camel.apache.org/schema/spring http://camel.apache.org/schema/spring/camel-spring.xsd ">

    <route id="HelloRequest">
        <from uri="activemq:queue:phucFrom" />
        <log message="Hello ${body}"/>
        <process ref="helloRequestProcess" />
        <process ref="helloRequestProcess2" />
        <to uri="jpa://com.example.activeMQ.model?namedQuery=tutorials.findAll" />
        <process ref="helloRequestProcess3" />
    </route>

    <route id="GetTutorialResponse">
        <from uri="activemq:queue:mssql.camel.jpa" />
        <log message="Hello ${body}"/>
        <to uri="jpa://com.example.activeMQ.model?namedQuery=tutorials.findAll" />
        <process ref="getTutorialsResponseProcess" />
        <process ref="createTutorialsRequestProcess" />
<!--        <to uri="jpa://com.example.activeMQ.model?nativeQuery=insert into tutorials(description, published, title) values (:#description, :#published, :#title)" />-->
    </route>

</routes>